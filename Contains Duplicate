class Solution:
    def containsDuplicate(self, lst):
        # initializing a map
        values = {}
        for i in nums: 
            # go through the values and if there is a number in there then we will return true 
            if i in values: 
                return True 
            else:
                values[i] = 1 # We've seen the value once
        return False

# Time Complexity: O(n)
# Space Complexity: O(n)